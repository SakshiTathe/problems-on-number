public class Series {
    public static void main(String[] args) {
        int num=18,sum=0,num2=num;
        while(num!=0){
            sum=sum+(num%10);
            num=num/10;
        }
        if((num2 % sum)==0){
            System.out.println(num2 +" is a Harshad number");
        }
        else System.out.println(num2 +" is a not Harshad number");
    }
}
